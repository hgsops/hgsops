        <!-- Projects Row -->
        <div class="row">       
            <div class="col-md-12 portfolio-item">
              <table id="table" class="table table-responsive table-bordered" cellspacing="0" width="100%">
                <thead>
                  <tr>
                    <th>Company Name</th>
                    <th>Unique transaction ID</th>
                    <th>Dollars Obligated</th>
                    <th>Current Completion Date</th>
                    <th>Ultimate Completion Date</th>
                    <th>Principle NAICS Code</th>
                    <th>Phone Number</th>
                    <th>8a Graduation Date</th>
                    <th>State</th>
                    <th>City</th>                
                  </tr>
                </thead>
                <tbody>
                <% for(var i = 0; i < (data.length/8); i++) {%>
                <tr>
                    <td><%= data[4+(i*8)] %></td>
                    <td><%= data[0+(i*8)] %></td>
                    <td><%='$'+ data[1+(i*8)] %></td>
                    <td><%= data[2+(i*8)] %></td>
                    <td><%= data[3+(i*8)] %></td>
                    <td><%= data[5+(i*8)] %></td>
                    <td><%= data[6+(i*8)] %></td>
                    <td><%= data[7+(i*8)] %></td>
                    <td>To Be Added</td>
                    <td>To Be Added</td>
                </tr>
                <% } %>
                </tbody>
              </table>                              
